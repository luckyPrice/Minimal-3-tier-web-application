.PHONY: compose-build
compose-build:
	docker compose build

.PHONY: compose-up
compose-up:
	docker compose up

.PHONY: compose-up-build
compose-up-build:
	docker compose up --build

.PHONY: compose-up-db
compose-up-db:
	docker compose up -d db

.PHONY: compose-down
compose-down:
	docker compose down